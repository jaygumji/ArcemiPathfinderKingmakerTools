{
  "executable": "Arcemi.SaveGameEditor",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "ArcemiSaveGameEditor",
  "author": "Arcemi",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.Arcemi.SaveGameEditor.app",
    "productName": "Arcemi Save Game Editor",
    "copyright": "Copyright Â© 2024",
    "buildVersion": "2.2.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}